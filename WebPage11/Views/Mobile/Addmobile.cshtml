@model WebPage11.Models.mobile
@{
    ViewBag.Title = "Addmobile";
}

<h2>Addmobile</h2>

<div class="row">
    <div class="col-md-12">
        @using (Html.BeginForm())
        {
        <div class="form-group">
            <label for="mobile">@Html.DisplayNameFor(obj => obj.mobile1)</label>
            @Html.TextBoxFor(obj => obj.mobile1, new { @class = "form-control" })
        </div>
        <div class="form-group">
            <label for="mobile">@Html.DisplayNameFor(obj => obj.model)</label>
            @Html.TextBoxFor(obj => obj.model, new { @class = "form-control" })
        </div>
        <div class="form-group">
            <label for="mobile">@Html.DisplayNameFor(obj => obj.price)</label>
            @Html.TextBoxFor(obj => obj.price, new { @class = "form-control" })
        </div>
        <div class="form-group">
            <label for="mobile">@Html.DisplayNameFor(obj => obj.description)</label>
            @Html.TextBoxFor(obj => obj.description, new { @class = "form-control" })
        </div>
        <input type="submit" value="Add" class="btn btn-primary"/>
        <input type="button" value="Reset" class="btn btn-primary"/>
        }
    </div>
</div>
